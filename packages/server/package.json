{
  "name": "@project/server",
  "version": "1.0.0",
  "description": "Here an example to configure your server and how to create your first controller.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm run tsc",
    "test": "npm run clean && yarn lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "tsc": "tsc --project tsconfig.json",
    "start": "nodemon --watch 'src/**/*.ts' -e ts --ignore 'node_modules/**/*' --exec ts-node src/index.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/common": "5.57.7",
    "@tsed/core": "5.57.7",
    "@tsed/di": "5.57.7",
    "@tsed/exceptions": "5.57.7",
    "@tsed/platform-express": "5.57.7",
    "@tsed/swagger": "5.57.7",
    "@tsed/typeorm": "5.57.7",
    "@types/dotenv": "^8.2.0",
    "@types/swagger-schema-official": "2.0.20",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "concurrently": "5.1.0",
    "cookie-parser": "1.4.4",
    "cross-env": "7.0.2",
    "dotenv": "^8.2.0",
    "env-var": "^6.2.0-alpha.0",
    "express": "4.17.1",
    "method-override": "^3.0.0",
    "pg": "^7.4.3",
    "node-uuid": "^1.4.8",
    "reflect-metadata": "^0.1.13",
    "sinon": "^9.0.2",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@types/chai": "4.2.10",
    "@types/chai-as-promised": "7.1.2",
    "@types/express": "4.17.3",
    "@types/http-proxy": "^1.16.2",
    "@types/mocha": "7.0.2",
    "@types/node": "13.9.1",
    "@types/request-promise": "^4.1.41",
    "@types/sinon": "7.5.2",
    "@types/sinon-chai": "3.2.3",
    "@types/supertest": "2.0.8",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "7.1.0",
    "nodemon": "^1.17.5",
    "nyc": "15.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.0",
    "sinon-chai": "3.5.0",
    "supertest": "4.0.2",
    "ts-node": "8.6.2",
    "ts-sinon": "^1.2.0",
    "tslint": "6.1.0",
    "typescript": "3.8.3"
  }
}